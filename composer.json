{
    "name": "mattmangoni/nova-recent-posts",
    "description": "A simple recent blog posts card for Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Mattmangoni\\RecentPosts\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mattmangoni\\RecentPosts\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
